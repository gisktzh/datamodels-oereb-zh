      Typ : OPTIONAL BL_Linien_Punkt_Typ;

      RG_GID: OPTIONAL GIDTYPE;
    NO IDENT

    END BL_Linien_Punkt;

    !! Punkt projektiert
    !!------------------

    TABLE BL_Linien_Punkt_PROJ =
      BFS_Nummer : CHMunicipalityCode;
      Gemeinde_Name : OPTIONAL TEXT*80;
      Rechtsstatus : RechtsstatusObjekt;
      Geometrie : ZHPoint;
      Nummer : OPTIONAL TEXT*12;
