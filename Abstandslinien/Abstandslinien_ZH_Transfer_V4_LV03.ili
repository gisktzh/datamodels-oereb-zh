      Wirksamkeit : OPTIONAL Wirksamkeit;
      Linie : ->BL_Linie_PROJ; !! {0..*}
      Geometrie : ZHDirectedLine;

      RG_GID: OPTIONAL GIDTYPE;
    NO IDENT

    !! constraints wie rechtsgueltig

