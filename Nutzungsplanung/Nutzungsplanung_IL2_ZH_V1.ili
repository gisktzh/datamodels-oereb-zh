         );


      !! Classes
      !!--------

      CLASS RV_Dokument =
         Art : MANDATORY DokumentArt;
         Titel : MANDATORY TEXT*80;
         OffiziellerTitel : TEXT*2400;
         Abkuerzung : TEXT*10;
         OffizielleNr : MANDATORY TEXT*20;
