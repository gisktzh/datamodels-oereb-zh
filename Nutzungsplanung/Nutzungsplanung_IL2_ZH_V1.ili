      CLASS GN_Zonenflaeche
            EXTENDS Nutzungsplanung_ZH_V1.Grundstrukturen.NP_Objekt =

         Rechtsstatus : MANDATORY RechtsstatusObjekt;

         Ausnuetzungsziffer_Min : Ausnuetzungsziffer;
